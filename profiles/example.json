{
  "$schema": "schema.json",
  "cssFile": "_font-face.scss",
  "urlPrefix": "/assets/fonts/",
  "chunk": [
    {
      "family": "Noto Sans JP",
      "weight": 400,
      "swap": true
    }
  ],
  "subset": [
    {
      "family": "Barlow",
      "weight": 500,
      "text": "0123456789",
      "swap": true
    },
    {
